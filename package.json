{
  "name": "node-preconditions",
  "version": "1.0.0",
  "description": "A preconditions package for node modules based on Google's Preconditions library",
  "private": false,
  "author": "Anshul Verma <anshul.verma86@gmail.com>",
  "main": "lib/main.js",
  "maintainers": [
    "Anshul Verma <anshul.verma86@gmail.com>"
  ],
  "scripts": {
    "test": "grunt travis",
    "prepublish": "cake build && cake docs"
  },
  "config": {
    "blanket": {
      "pattern": "src",
      "loader": "./node-loaders/coffee-script",
      "data-cover-never": "node_modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/anshulverma/node-preconditions"
  },
  "dependencies": {
    "coffee-script": "1.7.1"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "1.9.1",
    "docco": "0.6.3",
    "grunt": "^0.4.5",
    "grunt-blanket": "0.0.8",
    "grunt-coffeelint": "0.0.10",
    "grunt-mocha-cov": "^0.2.1",
    "grunt-mocha-test": "^0.11.0",
    "mocha": "1.20.1"
  },
  "bundledDependencies": [],
  "keywords": [
    "coffee-script",
    "cake",
    "preconditions"
  ],
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.1.65"
  },
  "directories": {
    "doc": "./docs",
    "lib": "./lib"
  }
